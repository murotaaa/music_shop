<div class="t_body">


 <div class="t_head">管理者ログイン画面</div>

 <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
  <div class="field">
    <%= f.label :email,"メールアドレス", class: 't_log-in-title1' %><br />
    <%= f.email_field :email, autocomplete: "email", class: 't_log-in-form' %>
  </div>

  <div class="field">
    <%= f.label :password,"パスワード", class: 't_log-in-title2' %><br />
    <%= f.password_field :password, autocomplete: "current-password", class: 't_log-in-form' %>
  </div>

  <div class="t_position">
    <%= f.submit "ログイン", class: "t_btn_btn-default2" %>
  </div>

 <% end %>

</div>
