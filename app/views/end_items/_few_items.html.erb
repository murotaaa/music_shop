<div id="few_items">
	<div class="col-xs-12">
		<h3>残り在庫あとわずか!!</h3>
	</div>
	<div class="row">
		<div class="col-xs-1">
			<%= link_to_prev_page @few_items, fa_icon("angle-double-left 5x"), param_name: :"few_page", remote: :true %>
		</div>
		<div class="col-xs-10">
			<div class="row">
				<ul>
					<%= render partial: "contents_of_each", collection: @few_items, as: :item %>
				</ul>
			</div>
		</div>
		<div class="col-xs-1">
			<%= link_to_next_page @few_items, fa_icon("angle-double-right 5x"), param_name: :"few_page", remote: :true %>
		</div>
	</div>
</div>